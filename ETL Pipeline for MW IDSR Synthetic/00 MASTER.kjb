    <entry>
      <name>SQL</name>
      <description/>
      <type>SQL</type>
      <attributes/>
      <sql>ALTER TABLE ${schema}_${version}.mw_idsr_synthetic_${version} RENAME COLUMN OCCU TO PATIENT_OCCUPATION;
ALTER TABLE ${schema}_${version}.mw_idsr_synthetic_${version} RENAME COLUMN SEX TO PATIENT_SEX;
ALTER TABLE ${schema}_${version}.mw_idsr_synthetic_${version} RENAME COLUMN NATL TO REPORT_COUNTRY;
ALTER TABLE ${schema}_${version}.mw_idsr_synthetic_${version} RENAME COLUMN REPR_NM TO PERSON_FORM_COMPLETE;
ALTER TABLE ${schema}_${version}.mw_idsr_synthetic_${version} RENAME COLUMN D_L_VACCI TO DATE_LAST_VACCINE;
ALTER TABLE ${schema}_${version}.mw_idsr_synthetic_${version} RENAME COLUMN VACC_NUM_DOSE TO VACCINE_DOSES_RECEIVED;
ALTER TABLE ${schema}_${version}.mw_idsr_synthetic_${version} RENAME COLUMN VACC_TYP_VAC TO VACCINE_NAME;
ALTER TABLE ${schema}_${version}.mw_idsr_synthetic_${version} RENAME COLUMN REPR_FACI TO REPORT_SITE;
ALTER TABLE ${schema}_${version}.mw_idsr_synthetic_${version} RENAME COLUMN D_ONSE TO DATE_ONSET_SYMPTOMS;
ALTER TABLE ${schema}_${version}.mw_idsr_synthetic_${version} RENAME COLUMN D_S_COLL TO DATE_SPECIMEN_COLLECTED;
ALTER TABLE ${schema}_${version}.mw_idsr_synthetic_${version} RENAME COLUMN D_S_SENT_LAB TO DATE_SPECIMEN_SENT_LAB;
ALTER TABLE ${schema}_${version}.mw_idsr_synthetic_${version} RENAME COLUMN TYP_CASE TO PATIENT_TYPE;
ALTER TABLE ${schema}_${version}.mw_idsr_synthetic_${version} RENAME COLUMN FIN_LAB_RESU TO LAB_RESULT;
ALTER TABLE ${schema}_${version}.mw_idsr_synthetic_${version} RENAME COLUMN CASE_F_O TO OUTCOME;
ALTER TABLE ${schema}_${version}.mw_idsr_synthetic_${version} RENAME COLUMN CASE_F_CLASS TO FINAL_CLASSIFICATION;
ALTER TABLE ${schema}_${version}.mw_idsr_synthetic_${version} RENAME COLUMN NEAR_LMK TO PATIENT_RESIDENCE;
ALTER TABLE ${schema}_${version}.mw_idsr_synthetic_${version} RENAME COLUMN D_FACI_NOTI_DIST TO DATE_FACILITY_NOTIFIED_DISTRICT;
ALTER TABLE ${schema}_${version}.mw_idsr_synthetic_${version} RENAME COLUMN D_SEEN_FACI TO DATE_HEALTH_FACILITY;
ALTER TABLE ${schema}_${version}.mw_idsr_synthetic_${version} RENAME COLUMN RECE_TRAV_HIST TO TRAVEL_DESTINATION;
ALTER TABLE ${schema}_${version}.mw_idsr_synthetic_${version} RENAME COLUMN TYP_REPR_DISE_COND TO DIAGNOSIS;
ALTER TABLE ${schema}_${version}.mw_idsr_synthetic_${version} RENAME COLUMN DIST_CASE_RESI_TYP TO PATIENT_AREA_TYPE;
ALTER TABLE ${schema}_${version}.mw_idsr_synthetic_${version} RENAME COLUMN REPR_DIST TO REPORT_DISTRICT;
ALTER TABLE ${schema}_${version}.mw_idsr_synthetic_${version} RENAME COLUMN D_LAB_SENT_RESU_DIST TO DATE_FORM_SENT_DISTRICT;

-- concatenating firstname and last name to create patient name 

ALTER TABLE ${schema}_${version}.mw_idsr_synthetic_${version} ADD COLUMN PATIENT_NAME character varying(100);
UPDATE ${schema}_${version}.mw_idsr_synthetic_${version} SET PATIENT_NAME = CONCAT (FN_CASE, ' ', LN_CASE);
ALTER TABLE ${schema}_${version}.mw_idsr_synthetic_${version}  DROP COLUMN FN_CASE, DROP COLUMN LN_CASE;

-- concatenating physical address, phone number and parent caretaker to create patient address

ALTER TABLE ${schema}_${version}.mw_idsr_synthetic_${version} ADD COLUMN PATIENT_ADDRESS character varying(100);
UPDATE ${schema}_${version}.mw_idsr_synthetic_${version} SET PATIENT_ADDRESS = CONCAT (PHYS_ADDR, ', ', PHON_NUM_CASE, ', ', PARE_CARET_NM);
ALTER TABLE ${schema}_${version}.mw_idsr_synthetic_${version}  DROP COLUMN PHYS_ADDR, DROP COLUMN PHON_NUM_CASE, DROP COLUMN PARE_CARET_NM;



-- calculating age columns
ALTER TABLE ${schema}_${version}.mw_idsr_synthetic_${version} ADD COLUMN AGE_YEARS INTEGER, ADD COLUMN AGE_MONTHS INTEGER, ADD COLUMN AGE_DAYS INTEGER;
UPDATE ${schema}_${version}.mw_idsr_synthetic_${version} SET AGE_YEARS = EXTRACT(YEAR FROM DOB), AGE_MONTHS = EXTRACT(MONTH FROM DOB), AGE_DAYS = EXTRACT(DAY FROM DOB);


ALTER TABLE ${schema}_${version}.mw_idsr_synthetic_${version} RENAME COLUMN DOB TO PATIENT_DOB;


-- travel history and travel destination
ALTER TABLE ${schema}_${version}.mw_idsr_synthetic_${version} ADD COLUMN TRAVEL_HISTORY character varying(3);

UPDATE ${schema}_${version}.mw_idsr_synthetic_${version} 
SET TRAVEL_HISTORY = CASE
                  WHEN TRAVEL_DESTINATION IS NOT NULL THEN 'Yes'
                  ELSE 'No'
              END;
-- Add Record Number
ALTER TABLE ${schema}_${version}.mw_idsr_synthetic_${version} ADD COLUMN recnr SERIAL;

-- add the new columns patient_town_city and patient_district
ALTER TABLE ${schema}_${version}.mw_idsr_synthetic_${version} ADD COLUMN PATIENT_TOWN_CITY character varying(50);
ALTER TABLE ${schema}_${version}.mw_idsr_synthetic_${version} ADD COLUMN PATIENT_DISTRICT character varying(50);
-- copy data from DIST_CASE_RESI to PATIENT_TOWN_CITY and PATIENT_DISTRICT
UPDATE ${schema}_${version}.mw_idsr_synthetic_${version} SET PATIENT_TOWN_CITY = DIST_CASE_RESI;
UPDATE ${schema}_${version}.mw_idsr_synthetic_${version} SET PATIENT_DISTRICT = DIST_CASE_RESI;

-- create the report province table


ALTER TABLE ${schema}_${version}.mw_idsr_synthetic_${version} ADD COLUMN REPORT_PROVINCE character varying(50);

UPDATE ${schema}_${version}.mw_idsr_synthetic_${version} 
SET REPORT_PROVINCE = 	CASE
							WHEN 
								REPORT_DISTRICT IN('MW-DE','MW-DO','MW-KS','MW-LI','MW-MC','MW-NK','MW-NU','MW-NI','MW-SA')
								THEN 'Central'
         					WHEN 
								REPORT_DISTRICT IN('MW-CT','MW-KR','MW-LK','MW-MZ','MW-NB','MW-RU')
								THEN 'Northern'
							WHEN 
								REPORT_DISTRICT IN('MW-BA','MW-BL','MW-CK','MW-CR','MW-MH','MW-MG','MW-MU','MW-MW','MW-NE','MW-NS','MW-PH','MW-TH','MW-ZO')
								THEN 'Southern'
              			END;
						
-- And for rec_identifier:
ALTER TABLE synthetic_v1.mw_idsr_synthetic_v1 ADD COLUMN rec_identifier VARCHAR(100);

UPDATE synthetic_v1.mw_idsr_synthetic_v1
		SET rec_identifier = 	to_char(date_health_facility, 'YYYY') || 
								'-' || 
                                LPAD(to_char(EXTRACT(WEEK FROM date_health_facility), 'FM9999'), 4, '0') || 
								'-' ||
                                '265-' || 
                                CASE report_province 
                                     WHEN 'Northern' THEN '001' 
                                     WHEN 'Central' THEN '002' 
                                     WHEN 'Southern' THEN '003' 
                                     ELSE '000' 
                                END || 
								'-' ||
                                report_district ||
								'-' ||
								'001-' || 
                                LPAD(recnr::TEXT, 5, '0')
        ;
</sql>
