ALTER TABLE ${cdm_schema}.location ADD CONSTRAINT xpk_location PRIMARY KEY ( location_id ) ;

ALTER TABLE ${cdm_schema}.location_history ADD CONSTRAINT xpk_location_history PRIMARY KEY ( location_history_id ) ; 

