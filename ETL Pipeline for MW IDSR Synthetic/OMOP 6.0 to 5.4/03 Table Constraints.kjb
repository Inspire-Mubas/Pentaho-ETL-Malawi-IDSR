      <sql>--postgresql OMOP CDM Indices

--  There are no unique indices created because it is assumed that the primary key constraints have been run prior to implementing indices.
--


--***********************

--Standardized clinical data

--***********************

CREATE INDEX idx_person_id  ON ${cdm_5_4_schema}.person  (person_id ASC);

CREATE INDEX idx_gender ON ${cdm_5_4_schema}.person (gender_concept_id ASC);

CREATE INDEX idx_observation_period_id_1  ON ${cdm_5_4_schema}.observation_period  (person_id ASC);


